while true do
    if _G.vexalotl_ForsakenScriptBool then
        if _G.vexalotl_KillerEspBool then
            for _, model in pairs(workspace.Players.Killers:GetChildren()) do
                if not model:FindFirstChild("vexalotl supreme") then
                    local highlight = Instance.new("Highlight")
                    highlight.Name = "vexalotl supreme"
                    highlight.Parent = model 
                    highlight.FillColor = Color3.new(1, 0, 0)
                end
            end
        end
        if _G.vexalotl_SurvivorEspBool then
            for _, model in pairs(workspace.Players.Survivors:GetChildren()) do
                if not model:FindFirstChild("vexalotl supreme") then
                    local highlight = Instance.new("Highlight")
                    highlight.Name = "vexalotl supreme"
                    highlight.Parent = model
                    highlight.FillColor = Color3.new(1, 1, 1)
                end
            end
        end
        if _G.vexalotl_GeneratorEspBool then
            for _, generator in pairs(workspace.Map.Ingame.Map:GetChildren()) do
                if generator.Name == "Generator" and not generator:FindFirstChild("vexalotl supreme") then
                    local highlight = Instance.new("Highlight")
                    highlight.Name = "vexalotl supreme"
                    highlight.Parent = generator
                    highlight.FillColor = Color3.new(1, 1, 1)
                    if _G.vexalotl_AutoCompleteGeneratorsBool then
                        generator:WaitForChild("Remotes"):WaitForChild("RE"):FireServer()
                    end
                end
            end
        end
        wait(1)
    end
end
