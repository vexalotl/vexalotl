local http_request

if syn then
    http_request = syn.request
elseif SENTINEL_V2 then
    http_request = function(tb)
        return {
            StatusCode = 200,
            Body = request(tb.Url, tb.Method, tb.Body or '')
        }
    end
else
    http_request = request
end

-- Function to get HWID from exploit headers
function get_exploit_hwid()
    local response = http_request({
        Url = 'https://httpbin.org/get',
        Method = 'GET'
    })

    local success, decoded_body = pcall(function()
        return game:GetService('HttpService'):JSONDecode(response.Body)
    end)

    if success and decoded_body and decoded_body.headers then
        local hwid_keys = {
            "Syn-Fingerprint",
            "Exploit-Guid",
            "Krnl-Hwid",
            "Sw-Fingerprint",
            "Wave-Fingerprint"
        }

        for _, key in ipairs(hwid_keys) do
            if decoded_body.headers[key] then
                return decoded_body.headers[key]
            end
        end
    else
        warn("Failed to decode JSON or missing headers")
    end

    return nil
end

-- Function to get Roblox Client ID
function get_roblox_client_id()
    return tostring(game:GetService("RbxAnalyticsService"):GetClientId())
end

-- Main function to get both HWID and Client ID
function get_hwid_and_client_id()
    local exploit_hwid = get_exploit_hwid()
    local client_id = get_roblox_client_id()
    return exploit_hwid, client_id
end

-- Get HWID and Client ID
local exploit_hwid, client_id = get_hwid_and_client_id()

-- Print HWID and Client ID
print("HWID:", exploit_hwid or "HWID not found")
print("Client ID:", client_id)
